# Set WXDIR for your system
WXDIR = $(WXWIN)

EXTRAINC=-I.\..\include
EXTRALIBS=$(WXDIR)\lib\ia$(LIBEXT).lib

PROGRAM=iatest
OBJECTS = $(PROGRAM).obj

!include $(WXDIR)\src\makeprog.vc

# For broken VC++ 4
noopt:
	cl @<<
$(CPPFLAGS2) /Od /c /Tp $(PROGRAM).cpp
<<

